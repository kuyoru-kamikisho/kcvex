{
  "name": "kcvex",
  "version": "1.7.31",
  "description": "以 精小而快速 的理念设计的高效率布局UI库，同时适用于vue2和vue3！",
  "private": false,
  "type": "module",
  "author": {
    "name": "kuyoru"
  },
  "license": "MIT",
  "keywords": [
    "ui library",
    "expandTransition",
    "v-expand-transition",
    "v-overlay",
    "vue-ui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kuyoru-kamikisho/kcvex"
  },
  "bugs": {
    "url": "https://github.com/kuyoru-kamikisho/kcvex/issues"
  },
  "files": [
    "./kc",
    "./json",
    "./README.md",
    "./LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./kc/v3/index.js"
    },
    "./v2": {
      "import": "./kc/v2/index.js"
    },
    "./v3": {
      "import": "./kc/v3/index.js"
    },
    "./style": {
      "import": "./kc/style.js"
    },
    "./directive": {
      "import": "./kc/directive/index.js"
    }
  },
  "main": "./kc/v3/index.js",
  "module": "./kc/v3/index.js",
  "packageManager": "yarn@4.4.1",
  "vetur": {
    "tags": "./json/tags.json",
    "attributes": "./json/attributes.json"
  }
}
